#include<iostream.h>
#include<conio.h>
void swap(int *a,int *b)
{
	int temp=*a;
	*a=*b;
	*b=temp;
}
int main()
{
	int x,y;
	clrcsr();
	cout<<"enter value a:-"<<endl;
	cout>>x;
	cout<<"enter value b:-"<<endl;
	cin>>y;
	swap(&x,&y);
	cout<<"a="<<x<<endl<<"b"<<y<<endl;
	getch();
	return 0;
}